{
  "name": "@trezoaplex-foundation/tpl-inscription",
  "version": "0.8.1",
  "description": "A sitple program for inscribing bytes on chain.",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.json",
    "build:docs": "typedoc",
    "test": "ava --timeout 600s --serial --no-worker-threads",
    "lint": "eslint --ext js,ts,tsx src",
    "lint:fix": "eslint --fix --ext js,ts,tsx src",
    "format": "prettier --check src test",
    "format:fix": "prettier --write src test"
  },
  "files": [
    "/dist/src"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "homepage": "https://trezoaplex.com",
  "repository": "https://github.com/metaplex-foundation/tpl-candy-guard.git",
  "author": "Trezoaplex Maintainers <contact@trezoaplex.com>",
  "license": "Apache-2.0",
  "dependencies": {
    "@trezoaplex-foundation/tpl-toolbox": "^0.8.0",
    "@trezoaplex-foundation/tpl-token-metadata": "^3.1.2"
  },
  "peerDependencies": {
    "@trezoaplex-foundation/umi": ">= 0.8.2 < 1"
  },
  "devDependencies": {
    "@ava/typescript": "^3.0.1",
    "@trezoaplex-foundation/umi": "^0.8.2",
    "@trezoaplex-foundation/umi-bundle-tests": "^0.8.2",
    "@trezoa/web3.js": "^1.73.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.46.1",
    "ava": "^5.1.0",
    "eslint": "^8.0.1",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "typedoc": "^0.23.16",
    "typedoc-plugin-expand-object-like-types": "^0.1.1",
    "typedoc-plugin-missing-exports": "^1.0.0",
    "typescript": "^4.6.2",
    "vercel": "^28.16.0",
    "p-map": "npm:@esm2cjs/p-map@^5.5.0"
  },
  "ava": {
    "typescript": {
      "compile": false,
      "rewritePaths": {
        "src/": "dist/src/",
        "test/": "dist/test/"
      }
    }
  },
  "packageManager": "pnpm@8.2.0"
}
